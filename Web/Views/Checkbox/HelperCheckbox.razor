@inherits HelperCheckboxBase

<MudCheckBox Color="BackgroundColor" UncheckedColor="UncheckedColor"
             Disabled="@(IsPlayerLocked || (!IsPreselectedOrAvailable && (!IsMemberPermitted || !IsEventInFuture)))"
             @onmousedown="ChangeHelpingEvent"
             @onclick:preventDefault
             @bind-Value="IsEntered"/>