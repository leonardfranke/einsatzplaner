@inherits HelperCheckboxBase

@if(IsPlayerLocked)
{
    <input type="checkbox" disabled  checked/>
}
else if (IsMemberPermitted || IsPreselectedOrAvailable)
{
    <InputCheckbox style=@($"accent-color: {BackgroundColor};") @onmousedown="ChangeHelpingEvent" @onclick:preventDefault @bind-Value="IsPreselectedOrAvailable" />
}